<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <welcome-file-list>
    <welcome-file>html/login.html</welcome-file>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>uploadImage</servlet-name>
  <servlet-class>com.eg.all_servlet.Upload</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>uploadImage</servlet-name>
  <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>dataCollector</servlet-name>
  <servlet-class>com.eg.all_servlet.DataCollector</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>dataCollector</servlet-name>
  <url-pattern>/data</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.eg.all_servlet.LoginValidator</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>imageProvider</servlet-name>
  <servlet-class>com.eg.all_servlet.Serve</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>imageProvider</servlet-name>
  <url-pattern>/serve</url-pattern>
  </servlet-mapping>
</web-app>